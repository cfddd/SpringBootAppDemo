<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.example.springbootappdemo.mapper.RoleMapper">
    <insert id="create" parameterType="com.example.springbootappdemo.entity.Role">
        INSERT INTO role (`name`, `key`, `sort`)
        VALUES (#{name}, #{key}, #{sort})
    </insert>
</mapper>